        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TARGIL210819"
        mc:Ignorable="d"
        Title="Targil3" Height="300" Width="300">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="95,75,0,0" TextWrapping="Wrap" Text="NAME" VerticalAlignment="Top" Width="120"/>
